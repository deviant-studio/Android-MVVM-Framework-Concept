<?xml version="1.0" encoding="utf-8"?>
<manifest package="ds.sample"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <application
        android:name="ds.sample.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name="ds.sample.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="ds.sample.view.TestActivity" />
        <activity android:name="ds.sample.view.EmptyActivity" />
        <activity android:name="ds.sample.view.ToolBarActivity" />
        <activity android:name="ds.sample.view.ViewPagerActivity" />
        <activity android:name="ds.sample.view.CustomComponentActivity" />

    </application>

</manifest>